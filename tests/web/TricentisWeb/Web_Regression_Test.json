{
  "test_suite": {
    "name": "WebTestingDemo E2E Regression Test Suite",
    "version": "1.0",
    "created_date": "2026-02-12",
    "description": "Comprehensive end-to-end regression test cases for Demo Web Shop covering complete user journey from registration to order completion",
    "base_url": "https://demowebshop.tricentis.com",
    "test_environment": "Demo Environment",
    "total_test_cases": 15
  },
  "test_data": {
    "dynamic_user_data": {
      "note": "Execute 'python db_query_api.py' to get latest registered user credentials",
      "placeholders": {
        "firstName": "{latestFirstName}",
        "lastName": "{latestLastName}",
        "email": "{latestEmail}",
        "password": "{latestPassword}",
        "gender": "{latestGender}",
        "uid": "{latestUserUID}"
      }
    },
    "test_products": {
      "product1": "Build your own cheap computer",
      "product2": "Simple Computer", 
      "product3": "Smartphone",
      "search_keywords": ["computer", "book", "jewelry", "electronics"]
    },
    "test_addresses": {
      "shipping_address": {
        "address": "123 Test Street",
        "city": "Test City",
        "zipCode": "12345",
        "country": "United States",
        "phone": "+1-555-123-4567"
      }
    }
