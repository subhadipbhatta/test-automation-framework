  "regression_test_cases": [
    {
      "test_id": "E2E_REG_001",
      "test_name": "Complete User Registration to First Purchase Journey",
      "priority": "Critical",
      "test_type": "End-to-End",
      "context_reference": "user_management_context.md, product_management_context.md, shopping_cart_context.md, checkout_context.md",
      "prompt_references": ["REG_001_Valid_Male_Registration", "LOGIN_001_Valid_Credentials", "SEARCH_001_Keyword_Search", "CART_001_Add_Single_Item", "CHECKOUT_002_Registered_User_Checkout"],
      "description": "Test complete user journey from registration to successful order placement",
      "preconditions": [
        "Website is accessible",
        "Database is available for validation",
        "Payment system is in demo mode"
      ],
      "test_steps": [
        {
          "step": 1,
          "action": "Navigate to registration page",
          "data": {
            "url": "https://demowebshop.tricentis.com/register"
          },
          "expected_result": "Registration page loads successfully"
        },
        {
          "step": 2,
          "action": "Complete user registration with valid data",
          "data": {
            "gender": "Male",
            "firstName": "AutoTest",
            "lastName": "User{timestamp}",
            "email": "autotest.{timestamp}@testmail.com",
            "password": "TestPass123!",
            "confirmPassword": "TestPass123!"
          },
          "expected_result": "User registered successfully, redirect to confirmation page"
        },
        {
          "step": 3,
          "action": "Validate user creation in database",
          "data": {
            "validation_query": "SELECT * FROM RegistrationInfo WHERE email = '{email}'"
          },
          "expected_result": "User record exists with encrypted password and unique UID"
        },
        {
          "step": 4,
          "action": "Login with newly registered credentials",
          "data": {
            "email": "{registered_email}",
            "password": "TestPass123!"
          },
          "expected_result": "Successful login, user session established"
        },
        {
          "step": 5,
          "action": "Search for product and add to cart",
          "data": {
            "search_term": "computer",
            "product": "{first_search_result}",
            "quantity": 1
          },
          "expected_result": "Product found and added to cart successfully"
        },
        {
          "step": 6,
          "action": "Complete checkout process",
          "data": {
            "checkout_type": "registered_user",
            "payment_method": "Credit Card"
          },
          "expected_result": "Order placed successfully with confirmation"
        }
      ],
      "validation_points": [
        "User registration data integrity",
        "Login session establishment",
        "Product search functionality", 
        "Cart management accuracy",
        "Order completion and confirmation"
      ],
      "estimated_duration": "8-10 minutes"
    },
    {
      "test_id": "E2E_REG_002", 
